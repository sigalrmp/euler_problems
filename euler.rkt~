#lang racket

(define (digits-h n d)
  (if (< n 1)
      d
      (digits-h (/ n 10) (+ d 1))))

(define (digits n)
  (digits-h n 0))

(printf "34: ~s\n" (digits 34))
(printf "213: ~s\n" (digits 213))
(printf "2: ~s\n" (digits 2))
(printf "91847390821748932107589743289582: ~s\n" (digits 91847390821748932107589743289582))

(define (fib-h l i)
  (if (digits (car l) >= 1000)
      i
      (fib-h (cons (+ (car l) (cadr l)) l)
             (+ i 1))))